{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-class-state": "warn",
		"no-jquery/no-sizzle": "warn",
		"no-shadow": "warn",
		"no-jquery/no-extend": "warn",
		"es-x/no-object-assign": "warn",
		"prefer-const": "warn",
		"no-jquery/no-done-fail": "warn"
	}
}
